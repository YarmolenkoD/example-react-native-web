    "@storybook/testing-library": "^0.0.13",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react-dom": "^18.0.6",
    "@types/react-native": "^0.69.3",
    "@types/react-test-renderer": "^18.0.0",
