    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.4.4"
