    "react-router-native": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@craco/craco": "^6.4.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-knobs": "^6.4.0",
